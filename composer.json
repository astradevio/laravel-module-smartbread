{
    "name": "astradevio/laravel-module-smartbread",
    "description": "Manage Laravel SmartBread on a laravel modules.",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Leandro Neves",
            "email": "leandro@astradev.io",
            "homepage": "https://www.astradev.io"
        }
    ],
    "homepage": "https://github.com/astradev-io/laravel-module-smartbread",
    "keywords": [
        "Laravel",
        "Laravel Modules",
        "Generator",
        "SmartBread",
        "CRUD",
        "CRUD Generator"
    ],
    "require": {
        "illuminate/support": "10.x|11.x|^12.0",
        "symfony/filesystem": "^6.0|^7.0",
        "symfony/finder": "^6.0|^7.0",
        "laravel/prompts": "^0.1.15|^0.3"
    },
    "autoload": {
        "psr-4": {
            "astradevio\\LaravelModuleSmartBread\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "astradevio\\LaravelModuleSmartBread\\SmartBreadServiceProvider"
            ],
            "aliases": {
                "SmartBreadGenerator": "astradevio\\LaravelModuleSmartBread\\Facades\\SmartBreadGenerator",
                "SmartBreadReplacer": "astradevio\\LaravelModuleSmartBread\\Facades\\SmartBreadReplacer",
                "SmartBreadService": "astradevio\\LaravelModuleSmartBread\\Facades\\SmartBreadService"
            }
        }
    }
}
